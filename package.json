{
  "name": "jsreport-chrome-pdf",
  "version": "0.3.1",
  "description": "jsreport recipe which is rendering pdf from html using headless chrome",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "mocha test --timeout 15000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-chrome-pdf",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-chrome-pdf.git"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "archiver": "2.1.1",
    "puppeteer": "1.0.0",
    "uuid": "3.2.1",
    "yauzl": "2.9.1"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-core": "jsreport/jsreport-core#2.0.0-beta",
    "jsreport-debug": "jsreport/jsreport-debug#2.0.0-beta",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "jsreport/jsreport-templates#2.0.0-beta",
    "mocha": "5.0.0",
    "should": "13.2.1",
    "standard": "10.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
